@{
    ViewData["Title"] = "Wedding Planner";
}
@model Wedding
<div class="jumbotron">
    <h1>Plan your wedding!</h1>
    <a href="/dashboard">Home</a>
    <a href="/logout">Logout</a>
</div>
<form action="create" method="POST">
    <div class="form-group">
        <label>Wedder One:</label>
        <input type="text" name="NameOne" placeholder="Wedder Name" class="form-control" />
        <span asp-validation-for="NameOne"></span>
    </div>
    <div class="form-group">
        <label>Wedder Two:</label>
        <input type="text" name="NameTwo" placeholder="Wedder Name" class="form-control" />
        <span asp-validation-for="NameTwo"></span>
    </div>
    <div class="form-group">
        <label>Date</label>
        <input type="date" name="Date" class="form-control" />
        <span asp-validation-for="Date"></span>
    </div>
    <div class="form-group">
        <label>Wedding Address:</label>
        <input type="text" name="Address" class="form-control" />
        <span asp-validation-for="Address"></span>
    </div>
    <input type="submit" value="Create Wedding" class="btn btn-success" />
    
</form>